package com.company;
        import java.util.Scanner;


public class Main {
    public static void main(String[] args) {
        String str = "капитель 98 фронтон 677";
        int lenght  = str.length() - str.replaceAll("\\d+", "").length();
        System.out.println("цифр в строке" + lenght);
    }
}
